    "e2e": "ng e2e",
    "e2eHeadless": "ng e2e --devServerTarget='' --protractorConfig=e2e/protractor.headless.conf.js",
    "protractor": "protractor e2e/protractor.conf.js",
    "protractorHeadless": "protractor e2e/protractor.headless.conf.js"
  },
  "private": true,
